#Even numbers
def evens():
	numbers=[]
	num1 = int(input("Enter first number: "))
	num2 = int(input("Enter second number: "))
	for num in range(num1, num2+1):
		if num % 2 == 0:
			numbers.append(num)
	return numbers
print ('Calling evens')
even_list=evens()
print (even_list)

#Reverse Even numbers
def reverse_evens():
	number = []
	num3 = int(input("Enter first number: "))
	num4 = int(input("Enter second number: "))
	for num in range(num3-1, num4):
		if num % 2 == 0:
			 number.append(num)
	return number
print("Calling Reverse Evens"
reverse_list=reverse_evens()
print(reverse_list)

